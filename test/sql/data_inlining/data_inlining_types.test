statement ok
BEGIN

statement ok
CREATE TABLE ducklake.extra_types AS FROM extra_types

query I nosort extra_types
FROM ducklake.extra_types
----

statement ok
COMMIT

query I nosort extra_types
FROM ducklake.extra_types
