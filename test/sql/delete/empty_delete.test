# name: test/sql/delete/empty_delete.test
# description: Test ducklake empty delete
# group: [delete]

require ducklake

require parquet

test-env DUCKLAKE_CONNECTION __TEST_DIR__/{UUID}.db

test-env DATA_PATH __TEST_DIR__


statement ok
ATTACH 'ducklake:${DUCKLAKE_CONNECTION}' AS ducklake (DATA_PATH '${DATA_PATH}/ducklake_empty_delete_files', CATALOG 'test', CREATE_IF_NOT_EXISTS true);

statement ok
CREATE TABLE ducklake.test AS SELECT i id FROM range(1000) t(i);

statement ok
BEGIN

query I
DELETE FROM ducklake.test WHERE id>10000
----
0

query II
SELECT COUNT(*), COUNT(*) FILTER(WHERE id%2=0) FROM ducklake.test
----
1000	500

statement ok
COMMIT

query II
SELECT COUNT(*), COUNT(*) FILTER(WHERE id%2=0) FROM ducklake.test
----
1000	500
