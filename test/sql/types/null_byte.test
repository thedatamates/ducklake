# name: test/sql/types/null_byte.test
# description: Test null bytes in strings
# group: [types]

require ducklake

require parquet

test-env DUCKLAKE_CONNECTION __TEST_DIR__/{UUID}.db

test-env DATA_PATH __TEST_DIR__


statement ok
ATTACH 'ducklake:${DUCKLAKE_CONNECTION}' AS ducklake (DATA_PATH '${DATA_PATH}/ducklake_null_byte', METADATA_CATALOG 'ducklake_meta', CATALOG 'test', CREATE_IF_NOT_EXISTS true);

statement ok
CREATE TABLE ducklake.tbl(s VARCHAR);

statement ok
INSERT INTO ducklake.tbl VALUES ('goo' || chr(0) || 'se'), ('hello');

query I
FROM ducklake.tbl
----
goo\0se
hello

query I
FROM ducklake.tbl WHERE s < 'hello'
----
goo\0se
